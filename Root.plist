<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<!-- Quality Button -->
		<dict>
			<key>cell</key>  <string>PSGroupCell</string>
			<key>label</key> <string>Quality Button</string>
		</dict>
		<dict>
			<key>cell</key>             <string>PSSwitchCell</string>
			<key>default</key>          <true/>
			<key>defaults</key>         <string>com.ps.youquality</string>
			<key>key</key>              <string>enabled</string>
			<key>label</key>            <string>Show quality button</string>
			<key>PostNotification</key> <string>com.ps.youquality/reload</string>
		</dict>
		<dict>
			<key>cell</key>             <string>PSSegmentCell</string>
			<key>defaults</key>         <string>com.ps.youquality</string>
			<key>key</key>              <string>position</string>
			<key>label</key>            <string>Quality button position</string>
			<key>default</key>          <integer>0</integer>
			<key>values</key>
			<array><integer>0</integer><integer>1</integer></array>
			<key>titles</key>
			<array><string>Top</string><string>Bottom</string></array>
			<key>PostNotification</key> <string>com.ps.youquality/reload</string>
		</dict>

		<!-- Volume Booster -->
		<dict>
			<key>cell</key>      <string>PSGroupCell</string>
			<key>label</key>     <string>Volume Booster</string>
			<key>footerText</key>
			<string>Adds a mic button beside the quality button. Tap to cycle gain levels; long-press to reset to 100%.</string>
		</dict>
		<dict>
			<key>cell</key>             <string>PSSwitchCell</string>
			<key>default</key>          <true/>
			<key>defaults</key>         <string>com.ps.youquality</string>
			<key>key</key>              <string>volBoostEnabled</string>
			<key>label</key>            <string>Enable Volume Booster</string>
			<key>PostNotification</key> <string>com.ps.youquality/reload</string>
		</dict>
		<dict>
			<key>cell</key>             <string>PSSegmentCell</string>
			<key>defaults</key>         <string>com.ps.youquality</string>
			<key>key</key>              <string>volGain</string>
			<key>label</key>            <string>Volume Gain</string>
			<key>default</key>          <real>100</real>
			<key>values</key>
			<array>
				<real>100</real><real>125</real><real>150</real>
				<real>175</real><real>200</real><real>250</real>
				<real>300</real><real>350</real><real>400</real>
			</array>
			<key>titles</key>
			<array>
				<string>100%</string><string>125%</string><string>150%</string>
				<string>175%</string><string>200%</string><string>250%</string>
				<string>300%</string><string>350%</string><string>400%</string>
			</array>
			<key>PostNotification</key> <string>com.ps.youquality/reload</string>
		</dict>
		<dict>
			<key>cell</key>             <string>PSEditTextCell</string>
			<key>defaults</key>         <string>com.ps.youquality</string>
			<key>key</key>              <string>volGainCustom</string>
			<key>label</key>            <string>Custom Gain (%)</string>
			<key>placeholder</key>      <string>Enter value between 100 and 400</string>
			<key>keyboard</key>         <string>UIKeyboardTypeNumberPad</string>
			<key>PostNotification</key> <string>com.ps.youquality/reload</string>
		</dict>

		<!-- Footer -->
		<dict>
			<key>cell</key>      <string>PSGroupCell</string>
			<key>footerText</key><string>YouQuality by PoomSmart</string>
		</dict>

	</array>
	<key>title</key>
	<string>YouQuality</string>
</dict>
</plist>